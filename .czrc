{
  "path": "cz-conventional-changelog",
  "disableScopeLowerCase": false,
  "disableSubjectLowerCase": false,
  "maxHeaderWidth": 100,
  "maxLineWidth": 100,
  "defaultType": "",
  "defaultScope": "",
  "defaultSubject": "",
  "defaultBody": "",
  "defaultIssues": "",
  "types": {
    "feat": {
      "description": "新功能",
      "title": "feat"
    },
    "fix": {
      "description": "bug 修复",
      "title": "fix"
    },
    "wip": {
      "description": "功能正在开发中,提交以切换其他分支进行其他开发",
      "title": "wip"
    },
    "style": {
      "description": "UI相关",
      "title": "style"
    },
    "refactor": {
      "description": "代码重构",
      "title": "refactor"
    },
    "docs": {
      "description": "文档更新",
      "title": "docs"
    },
    "build": {
      "description": "构建系统或者包依赖更新",
      "title": "build"
    },
    "ci": {
      "description": "CI/CD,自动构建,持续集成相关",
      "title": "ci"
    },
    "test": {
      "description": "测试用例相关",
      "title": "test"
    },
    "perf": {
      "description": "优化",
      "title": "perf"
    },
    "chore": {
      "description": "非src或者测试文件的更新",
      "title": "chore"
    },
    "revert": {
      "description": "回到上一个版本",
      "title": "revert"
    }
  }
}